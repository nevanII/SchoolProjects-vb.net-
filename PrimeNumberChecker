Imports System.Console


Module Module1
    Dim primecount, currentN As Integer
    Sub Main()
        ISPRIME()
        ReadKey()
    End Sub
    
    
    Sub ISPRIME()
        Dim c As Long
        Dim b As Long
        currentN = 1
        Do Until primecount = 1001
            currentN += 1
            c = Math.Round(Math.Sqrt(currentN)) + 1
            If currentN = 1 Then
                WriteLine("Not Prime")
            Else
                For b = 2 To c
                    If currentN Mod b = 0 And c <> b Then
                        WriteLine("Not Prime. Divisible by " & b)
                        Exit Sub
                    Else
                        If b = c Then
                            WriteLine("Prime")
                            Exit Sub
                        End If
                    End If
                Next b
            End If
            primecount += 1
        Loop
    End Sub
End Module
